{
  "editor.formatOnSave": true,
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "dart.projectSearchDepth": 3,
  "[dart]": {
    "editor.rulers": [
      0
    ],
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.dart_tool": true,
    "**/.metadata": true,
    "**/.packages": true,
    "**/.flutter-plugins": true,
    "**/.flutter-plugins-dependencies": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/generated_plugin_registrant.dart": true,
    "**/**.iml": true,
    "**/.idea": true,
    "**/.gitattributes": true,
    "**/build": true,
    "**/pubspec.lock": true,
    "**/**.freezed.dart": false,
    "**/**.g.dart": false,
    "**/LICENSE": true,
    "**/.gitignore": true,
    "**/.firebase": true,
    "**/.github": true,
    "**/.firebaserc": false,
    "**/firebase.json": false
  },
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true,
    "pubspec.lock": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit",
    "source.addMissingImports": "explicit"
  },
  "dart.flutterSdkPath": ".fvm/versions/3.22.0"
}